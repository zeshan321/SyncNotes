<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="note_pane" prefHeight="304.0" prefWidth="272.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.NoteViewController">
   <children>
      <Label fx:id="note_title" layoutX="14.0" layoutY="29.0" prefHeight="27.0" prefWidth="215.0" text="Label">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextArea fx:id="note_body" layoutX="14.0" layoutY="66.0" prefHeight="224.0" prefWidth="244.0" />
      <Button fx:id="note_close" layoutX="229.0" layoutY="28.0" mnemonicParsing="false" onAction="#note_close" text="X">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Label fx:id="label_save" alignment="CENTER" layoutX="-1.0" layoutY="-6.0" prefHeight="35.0" prefWidth="274.0" style="-fx-background-color: #000000;" text="Label" textFill="WHITE" visible="false" />
   </children>
</AnchorPane>
