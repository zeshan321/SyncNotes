<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="note_pane" prefHeight="291.0" prefWidth="272.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.NoteViewController">
   <children>
      <Label fx:id="note_title" layoutX="20.0" layoutY="14.0" prefHeight="27.0" prefWidth="215.0" text="Label">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextArea fx:id="note_body" layoutX="14.0" layoutY="53.0" prefHeight="224.0" prefWidth="244.0" />
      <Button fx:id="note_close" onAction="#note_close" layoutX="235.0" layoutY="15.0" mnemonicParsing="false" text="X">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
